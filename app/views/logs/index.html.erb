<h1>Listing logs</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Info</th>
      <th>Date</th>
      <th>Time</th>
      <th>Event Type</th>
      <th>User name</th>
      <th>Hide</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.id %></td>
        <td><%= log.info %></td>
        <td><%= log.date %></td>
        <td><%= log.time %></td>
        <td><%= log.eventType %></td>
        <td><%= log.user_name %></td>
        <td><%= log.hide %></td>
        <td><%= link_to 'Show', log %></td>
        <td><%= link_to 'Edit', edit_log_path(log) %></td>
        <td><%= link_to 'Destroy', log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Log', new_log_path %>
